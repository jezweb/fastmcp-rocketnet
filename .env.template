# Rocket.net API Testing Configuration
# Copy this file to .env and fill in your values

# Your bearer token from the Rocket.net API authentication endpoint
# Get this by calling POST /login with your username and password
ROCKETNET_BEARER_TOKEN=your_bearer_token_here

# A site ID for testing site-specific endpoints
# You can get this from the /sites endpoint
ROCKETNET_SITE_ID=your_site_id_here

# Optional: A backup ID for testing backup endpoints
ROCKETNET_BACKUP_ID=

# Optional: A domain ID for testing domain endpoints
ROCKETNET_DOMAIN_ID=

# Optional: An invoice ID for testing billing endpoints
ROCKETNET_INVOICE_ID=